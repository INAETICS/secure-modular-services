Bundle-Activator: org.amdatu.remote.discovery.slp.Activator
Bundle-Name: Amdatu Remote - Discovery (SLP)
Bundle-Version: 0.1.3

Import-Package: \
  sun.misc, \
  sun.reflect, \
  *

Private-Package: \
  org.amdatu.remote,\
  org.amdatu.remote.discovery,\
  org.amdatu.remote.discovery.slp,\
  org.apache.commons.lang3.*,\
  org.livetribe.slp.*

Provide-Capability: osgi.remoteserviceadmin.discovery; \
  protocols:List<String>="slp"; version:Version="1.1"